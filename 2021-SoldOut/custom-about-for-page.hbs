{{!< default}}

{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}
<main class="ha__main {{body_class}} custom-template custom-about-for-page">
  {{#post}}
    <div class="d-flex justify-content-center align-items-center flex-column about-me-home">
      <div class="col-12 col-sm-12 col-md-5 col-lg-5 col-xl-5 about-me">
        <div class="about-me__side about-me__side--front">
          <div class="about-me__cont">
            <span class="blue">alert</span><span>(<span class="green">'Hello World!'</span>)</span>
          </div>
        </div>
        <div class="about-me__side about-me__side--back">
          <!-- Back Content -->
          <div class="about-me__cta">
            <p>
              <span class="purple">const</span> aboutMe <span class="cyan">=</span> {
              <br />
              <span class="space red">name</span><span class="cyan">:</span> <span class="green">'Jaxson Wang'</span>,
              <br/>
              <span class="space red">email</span><span class="cyan">:</span> <span class="green">'i@iiong.com'</span>,
              <br/>
              <span class="space red">position</span><span class="cyan">:</span> <span class="green">'Web Front-End Developer'</span>,
              <br/>
              <span class="space red">website</span><span class="cyan">:</span> <span class="green">'https://iiong.com'</span>
              <br/> };
            </p>
          </div>
        </div>
      </div>
    </div>
    <div class="ha__main-article">
      <div class="container">
        <div class="ha__main-article-main my-5">
          <article class="ha__main-article-content ha__main-article-content-default">{{content}}</article>
        </div>
      </div>
    </div>
    {{> "site-to-top"}}
  {{/post}}
</main>
